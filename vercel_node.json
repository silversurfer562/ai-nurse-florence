{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@18.x",
      "maxDuration": 30,
      "memory": 512
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
