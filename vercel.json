{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "API_BEARER": "vercel-default-bearer",
    "CORS_ORIGINS": "https://*.vercel.app,http://localhost:3000"
  }
}