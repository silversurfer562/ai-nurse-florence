{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/static/assets/index.html"
    },
    {
      "source": "/chat",
      "destination": "/static/assets/chat.html"
    },
    {
      "source": "/api/v1/health",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/docs",
      "destination": "/api/index.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/index.py"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
