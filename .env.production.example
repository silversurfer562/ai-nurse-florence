## Example production environment for AI Nurse Florence
# Copy to .env.production and replace placeholders with real secrets

# Core Application
OPENAI_API_KEY=REPLACE_WITH_YOUR_OPENAI_KEY
USE_LIVE=1
API_BEARER=REPLACE_WITH_SECURE_BEARER_TOKEN
CORS_ORIGINS=https://ainurseflorence.com,https://api.ainurseflorence.com
LOG_LEVEL=INFO

# Database
DATABASE_URL=postgresql+asyncpg://user:password@postgres/dbname

# Redis
REDIS_URL=redis://redis:6379/0

# External APIs
NCBI_API_KEY=REPLACE_WITH_NCBI_API_KEY

# Security
JWT_SECRET_KEY=REPLACE_WITH_JWT_SECRET_KEY

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Monitoring
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=REPLACE_WITH_GRAFANA_PASSWORD

# Docker
DOCKER_REGISTRY=localhost
TAG=latest
