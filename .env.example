# API Configuration
API_BEARER=your-api-key-here
CORS_ORIGINS=http://localhost:3000,https://chat.openai.com,https://chatgpt.com

# OpenAI Configuration
OPENAI_API_KEY=your-openai-key-here

# Service Configuration
USE_LIVE=0
LOG_LEVEL=INFO

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Redis Configuration (optional for production)
REDIS_URL=redis://redis:6379/0

# Database Configuration (use PostgreSQL for production)
DATABASE_URL=sqlite+aiosqlite:///./test.db

# OAuth2 Settings for GPT Store
OAUTH_CLIENT_ID=your-oauth-client-id-here
OAUTH_CLIENT_SECRET=your-oauth-client-secret-here
JWT_SECRET_KEY=your-super-secret-jwt-key-at-least-32-characters-long

# Monitoring Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin