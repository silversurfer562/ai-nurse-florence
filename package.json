{
  "name": "ai-nurse-florence",
  "version": "2.0.0",
  "description": "Educational healthcare AI assistant providing evidence-based medical information for nursing professionals",
  "main": "index.html",
  "private": true,
  "keywords": [
    "healthcare",
    "nursing",
    "ai-assistant", 
    "medical-education",
    "clinical-decision-support",
    "fastapi",
    "evidence-based-medicine"
  ],
  "author": {
    "name": "AI Nurse Florence Development Team",
    "email": "support@ai-nurse-florence.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/ai-nurse-florence.git"
  },
  "homepage": "https://silversurfer562.github.io/ai-nurse-florence/",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "./run_dev.sh",
    "start": "uvicorn app:app --host 0.0.0.0 --port 8000",
    "build": "echo 'No build step required for static frontend'",
    "test": "pytest",
    "test:integration": "pytest -m integration",
    "test:unit": "pytest tests/unit/",
    "lint": "flake8 . && mypy .",
    "lint:fix": "black . && isort .",
    "format": "black . && isort . --profile black",
    "type-check": "mypy api/ routers/ services/ utils/",
    "security": "bandit -r api/ routers/ services/ utils/",
    "docs": "python -c 'import webbrowser; webbrowser.open(\"http://localhost:8000/docs\")'",
    "serve": "python -m http.server 8080",
    "docker:build": "docker build -t ai-nurse-florence .",
    "docker:run": "docker run -p 8000:8000 --env-file .env ai-nurse-florence",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "setup": "./setup.sh",
    "clean": "find . -type d -name '__pycache__' -exec rm -rf {} + && find . -name '*.pyc' -delete",
    "validate": "python -m py_compile api/index.py",
    "health": "curl -f http://localhost:8000/api/v1/health || exit 1"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "dependencies": {
    "http-proxy-middleware": "^3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.1.0"
  }
}
